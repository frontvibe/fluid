{
  "name": "fluid",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "shopify hydrogen build --codegen",
    "dev": "shopify hydrogen dev --codegen",
    "preview": "shopify hydrogen preview --build",
    "lint": "eslint --no-error-on-unmatched-pattern .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write --ignore-unknown .",
    "format:check": "prettier --check --ignore-unknown .",
    "typecheck": "react-router typegen && tsc --noEmit",
    "codegen": "shopify hydrogen codegen",
    "sanity:typegen": "npm run sanity:extract && npm run sanity:generate",
    "sanity:extract": "sanity schema extract --path ./types/sanity/schema.json",
    "sanity:generate": "sanity typegen generate",
    "create:initial": "sanity exec app/sanity/scripts/create-initial-documents.ts --with-user-token",
    "create:cors": "sanity exec app/sanity/scripts/create-cors-origin.ts --with-user-token",
    "create:token": "sanity exec app/sanity/scripts/create-viewer-token.ts --with-user-token"
  },
  "dependencies": {
    "@portabletext/react": "^6.0.2",
    "@radix-ui/react-icons": "^1.3.2",
    "@sanity/asset-utils": "^2.3.0",
    "@sanity/client": "^7.16.0",
    "@sanity/code-input": "^7.0.8",
    "@sanity/core-loader": "^2.0.6",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^2.0.3",
    "@sanity/language-filter": "^4.1.0",
    "@sanity/preview-url-secret": "^4.0.3",
    "@sanity/react-loader": "^2.0.7",
    "@sanity/ui": "^3.1.11",
    "@sanity/uuid": "^3.0.2",
    "@sanity/vision": "^5.12.0",
    "@sanity/visual-editing": "^5.2.1",
    "@shopify/hydrogen": "^2026.1.0",
    "@shopify/hydrogen-react": "^2026.1.0",
    "@shopify/mini-oxygen": "^4.0.0",
    "@shopify/polaris": "^13.9.5",
    "@shopify/remix-oxygen": "^3.0.2",
    "@tanem/react-nprogress": "^6.0.1",
    "@vercel/stega": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "color2k": "^2.0.3",
    "embla-carousel": "8.6.0",
    "embla-carousel-autoplay": "8.6.0",
    "embla-carousel-react": "8.6.0",
    "graphql": "^16.13.0",
    "graphql-tag": "^2.12.6",
    "groq": "^5.12.0",
    "isbot": "^5.1.35",
    "lodash": "^4.17.23",
    "lodash.get": "^4.4.2",
    "lucide-react": "^0.575.0",
    "motion": "^12.34.3",
    "nanoid": "^5.1.6",
    "pluralize-esm": "^9.0.5",
    "radix-ui": "latest",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "^7.13.1",
    "react-use": "^17.6.0",
    "sanity": "^5.12.0",
    "sanity-plugin-internationalized-array": "^4.0.3",
    "sanity-plugin-media": "^4.1.1",
    "styled-components": "^6.3.11",
    "tailwind-merge": "^3.5.0",
    "tiny-invariant": "^1.3.3",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.4",
    "@eslint/js": "^9.39.2",
    "@graphql-codegen/cli": "^6.1.2",
    "@portabletext/types": "^4.0.1",
    "@react-router/dev": "^7.13.1",
    "@react-router/fs-routes": "^7.13.1",
    "@shopify/cli": "3.90.1",
    "@shopify/hydrogen-codegen": "^0.3.3",
    "@shopify/oxygen-workers-types": "^4.2.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.2.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint": "^9.6.1",
    "@types/lodash": "^4.17.24",
    "@types/lodash.get": "^4.4.9",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.15.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "execa": "^9.6.1",
    "globals": "^17.3.0",
    "graphql-config": "^5.1.5",
    "hydrogen-sanity": "^6.1.1",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "schema-dts": "^1.1.5",
    "tailwindcss": "^4.2.1",
    "tailwindcss-animate": "^1.0.7",
    "type-fest": "^5.4.4",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@10.28.2",
  "browserslist": [
    "defaults"
  ],
  "eslintIgnore": [
    "build",
    "node_modules",
    "bin",
    "*.d.ts",
    "dist",
    "studio/*"
  ],
  "overrides": {
    "uuid": "^11.1.0"
  },
  "resolutions": {
    "uuid": "^11.1.0"
  },
  "pnpm": {
    "overrides": {
      "uuid": "^11.1.0"
    }
  }
}
